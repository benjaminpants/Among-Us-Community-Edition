using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using MoonSharp.Interpreter;
[MoonSharpUserData]
public class CE_FakePlayer2 : CE_LuaSpawnableObject
{
    public byte ColorId = 0;
    public byte HatId = 0;
    public string Name = "Bob";

    CE_FakePlayer()
    {
      
    }

    CE_FakePlayer(byte color, byte hat string name)
    {
        ColorId = color;
        HatId = hat;
        Name = name;
    }
}


